<h1>New Pets</h1>
<%= form_tag "/shelters/#{@shelter_id}/pets" do %>
<%= label_tag :name %>
<%= text_field_tag :name %>
<%= label_tag :age %>
<%= number_field_tag :age %>
<%= label_tag :description %>
<%= text_field_tag :description %>
<%= label_tag :sex %>
<%= text_field_tag :sex %>
<%= file_field :pet, :image %>
<%= submit_tag "Create Pet" %>
<% end %>
