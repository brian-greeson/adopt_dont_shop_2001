<h1>All Shelters</h1>
<a href="/shelters/new">Create Shelter</a>
<% @shelters.each do |shelter| %>
<%= link_to shelter.name, show_shelters_path(shelter), class: 'shelter' %>
<a href="/shelters/<%= shelter.id %>/edit">Update Shelter</a>
<%= link_to "Delete Shelter", delete_shelters_path(shelter), method: "delete" %>
<% end %>
